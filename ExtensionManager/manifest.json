{
	"name": "Extension Manager",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Manages Chrome extensions across all devices.",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon22.png",
			"38": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"128": "images/icon128.png"
	},
	"options_page": "options.html",
	"permissions": [
		"management",
		"storage"
	]
}