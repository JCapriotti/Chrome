{
	"name": "Extension Manager",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Manages Chrome extensions across devices. Allows you to maintain a list of preferred extensions, and pick what you want installed.",
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon22.png",
			"38": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"128": "images/icon128.png"
	},
	"options_page": "options.html",
	"permissions": [
		"management",
		"storage"
	]
}